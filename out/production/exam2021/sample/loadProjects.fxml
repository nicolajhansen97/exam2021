<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LoadProjects">
   <children>
      <Pane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: grey;">
         <children>
            <ListView fx:id="lProjectsView" layoutX="-2.0" prefHeight="400.0" prefWidth="410.0" />
            <Button fx:id="bLoadProjectName" layoutX="415.0" layoutY="343.0" mnemonicParsing="false" onAction="#getProjectName" prefHeight="31.0" prefWidth="178.0" text="Load project" />
            <Button fx:id="bDeleteProject" layoutX="431.0" layoutY="300.0" mnemonicParsing="false" onAction="#deleteProject" prefHeight="31.0" prefWidth="149.0" text="Delete project" />
         </children>
      </Pane>
   </children>
</AnchorPane>
